  procedures :
   -
     name : NouvelleListe
     procedure_id : 1199006258883004325
     type_code : 14
     code : |1-
      
      // Résumé : Crée une nouvelle liste
      //
      PROCEDURE NouvelleListe()
      
      sNom est une chaîne
      
      //1 : OK
      //2 : Annuler
      SELON Saisie(<§?10A3B8D4002B14EE§>,sNom )
      	// OK
      	CAS 1
      		SI sNom <> "" ALORS
      			// On teste si une liste avec le même nom n'existe pas déjà
      			HLitRecherche(Liste,Nom,sNom)
      			Si pas htrouve(Liste) ALORS
      				// Nouvelle liste
      				HRAZ(Liste)
      				Liste.Nom = sNom
      				Liste.PositionZR = ZR_Listes..Occurrence + 1
      				HAjoute(Liste)	
      				
      				// Ouverture de la liste
      				ouvrefenetremobile(FEN_Liste,Liste.IDListe)
      			SINON
      				Erreur("Une liste avec le même nom existe déjà.")
      				retour	
      			FIN
      			
      			
      		SINON
      			Erreur("Veuillez entrer un nom pour la liste.")
      		FIN
      	// Annuler
      	CAS 2
      		
      FIN
     type : 458752
   -
     name : EditeListe
     internal_properties : CAAAAAgAAABRI0cb/Sv6YjAbAFwBdSJJkofv3O8YDBl3FYb03GFJ4afKyDx95kXd9gVjukj2qwRtWOgoxt7Lb4t6IZKjLcmpIOR96MTlfc/t92O5ARcjz4DTIdoZLQufXlfmkpGJpceG8V4LaCGHTel8rfY=
     procedure_id : 1199007869496116241
     type_code : 14
     code : |1-
      
      // Résumé : Edite une liste
      //
      PROCEDURE EditeListe()
      
      sNom est une chaîne
      
      HLitRecherche(Liste,IDListe,ZR_Listes[ZR_Listes].ATT_ID)
      
      sNom = Liste.Nom
      
      //1 : OK
      //2 : Annuler
      SELON Saisie(<§?10A3B8D4002B14EE§>,sNom )
      	// OK
      	CAS 1
      		SI sNom <> "" ALORS
      			// Nouvelle liste
      			liste.Nom = sNom
      			HModifie(Liste)
      			
      			ZoneRépétéeAffiche(ZR_Listes)
      		SINON
      			Erreur("Veuillez entrer un nom pour la liste.")
      		FIN
      	// Annuler
      	CAS 2
      		
      FIN
     type : 458752
   -
     name : SauvePositions
     procedure_id : 1199008969005967868
     type_code : 14
     code : |1-
      
      // Résumé : Sauve les positions des listes dans la base en fonction de la ZR
      // 
      PROCEDURE SauvePositions()
      
      POUR i = 1 _a_ ZR_Listes..Occurrence
      	HLitRecherche(Liste,IDListe,ZR_Listes[i].ATT_ID)
      	SI HTrouve(Liste) ALORS
      		Liste.PositionZR = i
      		HModifie(Liste)
      	FIN
      FIN
      
      ZoneRépétéeAffiche(ZR_Listes)
     type : 458752
   -
     name : RafraîchitListes
     procedure_id : 1207521224819102184
     type_code : 14
     code : |1-
      // Résumé : Rafraîchit la ZR des listes
      //
      PROCEDURE RafraîchitListes()
      
      // Sauve la position des listes
      SauvePositions()
      
      // Rafraîchit la ZR
      ZoneRépétéeAffiche(ZR_Listes,taRéExecuteRequete)
     type : 458752
